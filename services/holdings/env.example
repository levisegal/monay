# Plaid API credentials from dashboard.plaid.com > Developers > Keys
MONAY_HOLDINGS_PLAID_CLIENT_ID=
MONAY_HOLDINGS_PLAID_SECRET=

# sandbox or production
MONAY_HOLDINGS_PLAID_ENV=sandbox

# Required for OAuth institutions (Schwab, etc.) in production - must be HTTPS
# Use ngrok for local dev: ngrok start monay --config ngrok.yml
# MONAY_HOLDINGS_PLAID_REDIRECT_URI=https://your-domain.ngrok-free.dev/

# ngrok authtoken from dashboard.ngrok.com/get-started/your-authtoken
NGROK_AUTHTOKEN=

# Server config
MONAY_HOLDINGS_LISTEN_ADDR=:8888
MONAY_HOLDINGS_LOGGING_LEVEL=info
